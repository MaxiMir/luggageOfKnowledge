{
  "expo": {
    "name": "Self Blog", // название приложения
    "slug": "rn-self-posts", // id приложения в expo
    "privacy": "public", // могут ли другие скачивать
    "sdkVersion": "35.0.0",
    "platforms": [ // на какие платформы
      "ios",
      "android"
    ],
    "version": "1.0.0", // вручную прописываем версию (мажорная | минорная | патч )
    "orientation": "default", // для вращения приложения
    "icon": "./assets/icon.png", // иконка приложения 1024*1024px
    "splash": { // загрузка приложения
      "image": "./assets/splash.png", // из документации 1242*2436px
      "resizeMode": "contain",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [ // все статические файлы идут с приложением
      "**/*"
    ],
    "ios": { // параметры для iOS
      "supportsTablet": true
    },
    "android": { // параметры для Android

    }
  }
}
