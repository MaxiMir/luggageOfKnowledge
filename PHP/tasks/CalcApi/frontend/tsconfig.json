{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": [
      "es2016",
      "es2017",
      "dom",
      "scripthost",
      "dom.iterable"
    ],
    "outFile": "./build/api-calc.js",
    "removeComments": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "./node_modules"
  ]
}
